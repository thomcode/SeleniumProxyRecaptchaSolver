{
   "action": {
      "default_icon": "icon/16.png",
      "default_popup": "popup.html",
      "default_title": "NopeCHA: CAPTCHA Solver"
   },
   "background": {
      "service_worker": "background.js",
      "type": "module"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "eventhook.js", "captcha/hcaptcha.js" ],
      "matches": [ "*://*.hcaptcha.com/captcha/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "eventhook.js", "captcha/recaptcha.js" ],
      "matches": [ "*://*.google.com/recaptcha/*", "*://*.recaptcha.net/recaptcha/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "captcha/funcaptcha.js" ],
      "matches": [ "*://*.arkoselabs.com/fc/*", "*://*.funcaptcha.com/fc/*", "*://*/fc/assets/*" ],
      "run_at": "document_end"
   }, {
      "all_frames": true,
      "js": [ "eventhook.js", "captcha/turnstile.js" ],
      "matches": [ "*://challenges.cloudflare.com/cdn-cgi/challenge-platform/h/b/turnstile/if/ov2/av0/*", "*://challenges.cloudflare.com/cdn-cgi/challenge-platform/h/g/turnstile/if/ov2/av0/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": false,
      "js": [ "pages/setup.js" ],
      "matches": [ "*://nopecha.com/setup" ],
      "run_at": "document_end"
   }, {
      "all_frames": true,
      "js": [ "pages/integrate.js" ],
      "matches": [ "*://nopecha.com/integrate" ],
      "run_at": "document_end"
   }, {
      "all_frames": false,
      "js": [ "pages/funcaptcha-demo.js" ],
      "matches": [ "*://nopecha.com/demo/funcaptcha" ],
      "run_at": "document_end"
   }, {
      "all_frames": true,
      "js": [ "captcha/awscaptcha.js", "captcha/geetest.js", "captcha/lemincaptcha.js", "captcha/textcaptcha.js", "captcha/perimeterx.js" ],
      "match_about_blank": true,
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   } ],
   "description": "Automatically solve reCAPTCHA, hCaptcha, FunCAPTCHA, AWS WAF, text CAPTCHA, and more using AI.",
   "host_permissions": [ "\u003Call_urls>" ],
   "icons": {
      "128": "icon/128.png",
      "16": "icon/16.png",
      "32": "icon/32.png",
      "48": "icon/48.png"
   },
   "key": "",
   "manifest_version": 3,
   "minimum_chrome_version": "100.0",
   "name": "NopeCHA: CAPTCHA Solver",
   "permissions": [ "declarativeNetRequest", "storage", "scripting" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.4.12",
   "version_name": "v0.4.12"
}
